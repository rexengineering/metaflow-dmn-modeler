{
  "name": "vs-code-dmn-modeler",
  "displayName": "vs-code-dmn-modeler",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dmn",
    "onCommand:extension.dmn-modeler.edit",
    "onWebviewPanel:dmn-modeler.editing"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dmn",
        "aliases": [
          "DMN",
          "dmn"
        ],
        "extensions": [
          ".dmn"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.dmn-modeler.edit",
        "title": "Edit DMN diagram",
        "icon": "$(editor-layout)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == dmn",
          "command": "extension.dmn-modeler.edit",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == dmn",
          "command": "extension.dmn-modeler.edit",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "run-s compile:clean compile:ts compile:assets",
    "compile:ts": "tsc -p ./",
    "compile:clean": "shx rm -rf out",
    "compile:assets": "shx cp -r src/assets out",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "dmn-js": "^10.1.0"
  }
}
